<?xml version="1.0"?>
<mapping name="mod" class="com.kcube.enh.mod.EnhMod">
	<bean class="com.kcube.enh.mod.EnhMod">
		<value name="id" />
		<value name="enhId" />
		<value name="title" />
		<value name="itemTitle" />
		<value name="pacName" />
		<value name="seqOrder" />
		<value name="content" />
		<value name="fileExt" />
		<value name="rgstUser" />
		<value name="lastUser" />
		<value name="rgstDate" />
		<value name="lastUpdt" />
		<value name="attachments" source="com.kcube.enh.mod.EnhMod$Attachment" />
		<value name="currentMember"/>
		<value name="currentOwner"/>
		<value name="currentModMember"/>
		<value name="currentParentAuthor"/>
	</bean>
	<bean class="com.kcube.sys.usr.User">
		<value name="id" method="userId" />
		<value name="name" />
		<value name="displayName" />
	</bean>
	<bean class="com.kcube.enh.mod.EnhMod$Attachment">
		<value name="id" />
		<value name="filename" />
		<value name="path" />
		<value name="type" />
		<value name="method" />
		<value name="size" method="filesize" />
	</bean>
</mapping>